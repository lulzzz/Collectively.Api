{
	"app":{
		"resetPasswordUrl": "http://localhost:9000/set-new-password"
	},
  "feature": {
		"resolveRemarkPhotoRequired": false,
		"resolveRemarkLocationRequired": false
  },
  "jwtToken": {
    "useRsa": true,
    "useRsaFilePath": true,
		"rsaPublicKeyXML": "rsa-public-key.xml",
		"issuer": "http://localhost:5000",
		"validateIssuer": true
  },
  "rawRabbit": {
		"username": "guest",
		"password": "guest",
		"virtualHost": "/",
		"port": 5672,
		"hostnames": [ "rabbitmq" ],
		"requestTimeout": "00:00:10",
		"publishConfirmTimeout": "00:00:01",
		"recoveryInterval": "00:00:10",
		"persistentDeliveryMode": true,
		"autoCloseConnection": true,
		"automaticRecovery": true,
		"topologyRecovery": true,
		"exchange": {
				"durable": true,
				"autoDelete": false,
				"type": "Topic"
		},
		"queue": {
				"autoDelete": false,
				"durable": true,
				"exclusive": false
		}
  },
	"services": [
		{
			"title": "storage-service",
			"name": "storage-service:10000",
			"username": "user",
			"password": "secret",
			"cacheExpiry": "01:00:00:00",
			"retryCount": 10,
			"retryDelayMilliseconds": 1000
    },
    {
      "title": "users-service",
      "name": "users-service:10002",
      "username": "user",
      "password": "secret"
    }
	]
}
