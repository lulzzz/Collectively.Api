{
	"app":{
		"avatarUrl": "https://s3.eu-central-1.amazonaws.com/becollectively/local/avatars/{0}_avatar.jpg",
		"resetPasswordUrl": "http://localhost:9000/set-new-password",
		"validateAccountState": true
	},
	"exceptionless": {
	  "enabled": false,
	  "apiKey": ""
  },
	"feature": {
		"resolveRemarkPhotoRequired": false,
		"resolveRemarkLocationRequired": false
	},
  "jwtToken": {
    "useRsa": true,
    "useRsaFilePath": true,
		"rsaPublicKeyXML": "rsa-public-key.xml",
		"issuer": "http://localhost:5000",
		"validateIssuer": true
  },
	"rawRabbit": {
		"username": "guest",
		"password": "guest",
		"virtualHost": "/",
		"port": 5672,
		"hostnames": ["localhost"],
		"requestTimeout": "00:00:10",
		"publishConfirmTimeout": "00:00:01",
		"recoveryInterval": "00:00:10",
		"persistentDeliveryMode": true,
		"autoCloseConnection": true,
		"automaticRecovery": true,
		"topologyRecovery": true,
		"exchange": {
				"durable": true,
				"autoDelete": false,
				"type": "Topic"
		},
		"queue": {
				"autoDelete": false,
				"durable": true,
				"exclusive": false
		}
	},
	"redis": {
		"enabled": true,
		"database": 0,
		"connectionString": "127.0.0.1"
	},
	"services": [
		{
			"title": "storage-service",
			"name": "localhost:10000",
			"username": "user",
			"password": "secret",
			"cacheExpiry": "01:00:00:00",
			"retryCount": 1,
			"retryDelayMilliseconds": 500
    },
    {
      "title": "users-service",
			"name": "localhost:10002",
			"noAuth": true
    }  
	]
}